<html>
    <head>
    <script>
    function Point(x1,y1,x2,y2) 
    {
        this.x1=x1;
        this.y1=y1;
        this.x2=x2;
        this.y2=y2;
        this.distance=function(){
return Math.sqrt(Math.pow(this.x2-this.x1,2)+Math.pow(this.y2-this.y1,2));};
        
    }
var Dis=new Point(3,5,6,9);
console.log(Dis.distance());
</script>
</head>
<body>
</body>
</html>
